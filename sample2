//Global and local variables
#include <stdio.h>
void sample();	//function (User define function) //proto-typing
int x=14;	//Global Variable

int main()
{
	int y=23; //Local Variable
	printf ("\n x = %d and y = %d",x,y);
	sample();	//calling the function
	return 0;
}
void sample()
{
	int y=25;		//local variable to the function sample
	printf ("this is function sample\n");
	printf ("\n x = %d and y = %d",x,y);
}
