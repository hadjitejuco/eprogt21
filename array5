//linear search
#include <stdio.h>
int main(){		
	int num[100],i,n,x=0,y=0;
	printf ("Enter n: ");
	scanf ("%d",&n);
	printf ("Enter %d numbers\n",n);
	for (i=0; i<n;i++){
		scanf ("%d",&num[i]);
	}
	printf ("Enter number to search ");
	scanf ("%d",&x);
	for (i=0; i<n;i++){
		if (num[i]==x){
			printf ("\nNumber %d is present at pos %d", x,i+1);
			y=1;
		}
	}
	if (y==0)
		printf ("number not found");
	return 0;
}
