/*
	Year			Level
	1				Freshmen
	2				Sophomore
	3				Junior
	4				Senior
	
	> others
*/
#include <stdio.h>
int main(){
	int yr;		
	printf ("Enter year ");
	scanf ("%d",&yr);
	if (yr==1){
		printf ("Freshmen");
	}else if(yr==2){
		printf ("Sophomore");
	}else if(yr==3){
		printf ("Junior");
	}else if(yr==4){
		printf ("Senior");
	}else{
		printf ("others");
	}
	return 0;
}
