//count number 1 - 10
#include <stdio.h>
int main(){
	int n,i; //n->enter number, i->loop
	printf ("Enter n:");	         //n=10   i      i<=n  p  i++
	scanf ("%d",&n);		 //	  1	  t    1   2
	//   (1)   (2)  (4)		 //		  t    2   3 
	for (i=1; i<=n;i++){             //               t    9   10 
		printf ("%4d",i); //(3)                   t    10  11
	}				//		  f
	return 0;
}
