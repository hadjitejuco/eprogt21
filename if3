//determine if input number is odd or even
// %  -> a%2==0 -> even, a%2==1 -> odd number
#include <stdio.h>  (if else statement)
int main()
{
	int a;
	printf ("enter a number ");
	scanf ("%d",&a);
		if (a%2==0){
			printf ("Number is even"); 	//true
		}
		else{
			printf ("Number is odd");	//false
		}
	return 0;
}
