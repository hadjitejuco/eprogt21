//Enter n: 5
//Sum of numbers from 1 to 5 = 15
//1+2+3+4+5 = 15
#include <stdio.h>
int main(){
	int n,i,s=0; 
	printf ("Enter n:");	
	scanf ("%d",&n);		
	//   (1)   (2)  (4)		 
	for (i=1; i<=n;i++){   
		//s=s+i;	//accumulator 	
		s+=i;
	}		
	printf ("the sum from 1 to %d is %d",n,s);     			
	return 0;
}
