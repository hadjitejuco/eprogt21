#include <stdio.h>
int main(){			//num[0]  num[1]
	int num[100],i,n;
	printf ("Enter n: ");
	scanf ("%d",&n);
	printf ("Enter %d numbers\n",n);
	for (i=0; i<n;i++){
		scanf ("%d",&num[i]);		// 1 2 3 4 5
	}
	printf ("List of numbers\n");
	for (i=n-1;i>=0;i--){
		printf ("%4d",num[i]);		// 5 4 3 2 1
	}
	return 0;
}
