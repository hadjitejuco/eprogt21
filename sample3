//scanf()
#include <stdio.h>
int main()
{
	int a,b;
	float c;
	//printf ("Enter value for A \n");
	//scanf ("%d",&a);
	//printf ("Enter value for B \n");
	//scanf ("%d",&b);
	printf ("Enter value for A and B \n");
	scanf ("%d%d",&a,&b);
	printf ("Enter value for C \n");
	scanf ("%f",&c);
	printf ("\n\nValues of A is %d, B is %d and C is %.2f",a,b,c);
	return 0;
}
