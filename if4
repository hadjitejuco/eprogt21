/*	nested if 
	Range			Grade
	90-100			A
	80-89			B
	70-79			C
	60-69			D
	below 			F	
*/
#include <stdio.h>
int main()
{
	int g;
	printf ("Enter grade ");
	scanf ("%d",&g);
	if ((g>=90)&&(g<=100)){
		printf ("A");
	}else if ((g>=80)&&(g<=89)){
		printf ("B");
	}else if ((g>=70)&&(g<=79)){
		printf ("C");
	}else if ((g>=60)&&(g<=69)){
		printf ("D");
	}else if ((g>=50)&&(g<=59)){
		printf ("F");
	}else{
		printf ("out of range");
	}
	return 0;
}



